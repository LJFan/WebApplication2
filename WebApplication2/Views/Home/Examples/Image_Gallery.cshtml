@{
    var images = new (string, string)[]
    {
        ("1", "images/1.jpg"),
        ("2", "images/2.jpg"),
        ("3", "images/3.jpg"),
        ("4", "images/4.jpg"),
        ("5", "images/5.jpg"),
        ("6", "images/6.jpg"),
        ("7", "images/7.jpg"),
        ("8", "images/8.jpg"),
        ("9", "images/9.jpg"),
        ("10", "images/10.jpg"),
                    };
}

<div class="row">
    @foreach (var image in images)
    {
        <div class="text-center col-xl-2 col-lg-3 col-md-4 col-sm-6 col-12 p-1">
            <div style="border: solid; border-width: 1px">
                <a href="~/@image.Item2">
                    <p>@image.Item1</p>
                    <img class="img-fluid" src="~/@image.Item2" style="height: 150px; width: auto;"/>
                </a>
            </div>
        </div>
    }
</div>